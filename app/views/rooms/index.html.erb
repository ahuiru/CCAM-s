<div class="dm-list">
  <% @anotherEntries.each do |e| %>
    <%= link_to room_path(e.room.id) do %>
    <div class="columns is-vcentered is-centered partner">
      <div class="column is-3 image is-inline-block">
        <%= image_tag e.user.image, class: 'image is-64x64 is-rounded' if @user.image.present? %>
      </div>
      <div class="column is-3">
        <%= e.user.name %><br>
      </div>
      <div class="column is-3">
        <%= Message.find_by(id: e.room.message_ids.last)&.content %>
      </div>
    </div>
    <hr>
    <% end %>
  <% end %>
</div>
